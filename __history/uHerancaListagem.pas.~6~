  FrmHerancaListagem: TFrmHerancaListagem;

implementation

{$R *.dfm}

procedure TFrmHerancaListagem.btnApagarMouseEnter(Sender: TObject);
begin
  inherited;
  ButtonMouseEnter(Sender, 3);
end;

